<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:skin="http://schemas.android.com/android/skin"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    tools:ignore="all">    <TextView        android:id="@+id/tv_news_label"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:layout_marginRight="6dp"        android:gravity="center"        android:textSize="9sp"/>    <TextView        android:id="@+id/newsfeed_item_source"        style="@style/ItemTagStyle"        android:layout_marginRight="6dp"        android:layout_toRightOf="@id/tv_news_label"        skin:skin_attrs="textColor=item_tag_color"/>    <TextView        android:id="@+id/tv_news_popularity"        style="@style/ItemTagStyle"        android:layout_marginRight="6dp"        android:layout_toRightOf="@id/newsfeed_item_source"        skin:skin_attrs="textColor=item_tag_color"/>    <TextView        android:id="@+id/tv_news_time"        style="@style/ItemTagStyle"        android:background="@color/colorAccent"        android:layout_toRightOf="@id/tv_news_popularity"        skin:skin_attrs="textColor=item_tag_color"/></RelativeLayout>